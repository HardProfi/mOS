local cpm = require("component")
local gpu = cpm.gpu
local os = require("os")
local Ws,Hs = gpu.getResolution()
local math = require("math")
local string = require("string")
local welcomeText = "Загрузка mOS!"
gpu.set(Ws/2-math.floor(string.len(welcomeText)/2),math.ceil(Hs/2), welcomeText)
os.sleep(2)
